{
	"name": "generate-infinity",
	"displayName": "GI",
	"description": "Empowering developers to build beyond boundaries with AI-driven automation and future-ready architectures. Turn complex code into limitless innovation—where every line scales infinitely.",
	"version": "1.0.5",
	"icon": "assets/icons/icon.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0"
	},
	"author": {
		"name": "IT Group."
	},
	"license": "Apache-2.0",
	"publisher": "IT",
	"repository": {
		"type": "git",
		"url": "https://github.com/cline/cline"
	},
	"homepage": "https://cline.bot",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Testing"
	],
	"keywords": [
		"ai",
		"coding",
		"test"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "claude-devS-ActivityBar",
					"title": "GI",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"views": {
			"claude-devS-ActivityBar": [
				{
					"type": "webview",
					"id": "claude-devS.SidebarProvider",
					"name": "",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"commands": [
			{
				"command": "gi.plusButtonClicked",
				"title": "新任务",
				"icon": "$(add)"
			},
			{
				"command": "gi.mcpButtonClicked",
				"title": "MCP扩展",
				"icon": "$(server)"
			},
			{
				"command": "gi.historyButtonClicked",
				"title": "历史",
				"icon": "$(history)"
			},
			{
				"command": "gi.popoutButtonClicked",
				"title": "在编辑区打开",
				"icon": "$(link-external)"
			},
			{
				"command": "gi.settingsButtonClicked",
				"title": "设置",
				"icon": "$(settings-gear)"
			},
			{
				"command": "gi.haierRAGCountClicked",
				"title": "Open RAG ACCount",
				"icon": "$(database)"
			},
			{
				"command": "gi.accountLoginClicked",
				"title": "Open ACCount",
				"icon": "$(account)"
			},
			{
				"command": "gi.openInNewTab",
				"title": "打开新选项卡",
				"category": "Cline"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "gi.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == claude-devS.SidebarProvider"
				},
				{
					"command": "gi.mcpButtonClicked",
					"group": "navigation@2",
					"when": "view == claude-devS.SidebarProvider"
				},
				{
					"command": "gi.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == claude-devS.SidebarProvider"
				},
				{
					"command": "gi.popoutButtonClicked",
					"group": "navigation@4",
					"when": "view == claude-devS.SidebarProvider"
				},
				{
					"command": "gi.settingsButtonClicked",
					"group": "navigation@5",
					"when": "view == claude-devS.SidebarProvider"
				},
				{
					"command": "gi.accountLoginClicked",
					"group": "navigation@5",
					"when": "view == claude-devS.SidebarProvider"
				}
			]
		},
		"configuration": {
			"title": "GI",
			"properties": {
				"GI.vsCodeLmModelSelector": {
					"type": "object",
					"properties": {
						"vendor": {
							"type": "string",
							"description": "语言模型的供应商（例如 copilot）"
						},
						"family": {
							"type": "string",
							"description": "语言模型的系列（例如 gpt-4）"
						}
					},
					"description": "VSCode 语言模型 API 的设置"
				},
				"GI.mcp.mode": {
					"type": "string",
					"enum": [
						"full",
						"server-use-only",
						"off"
					],
					"enumDescriptions": [
						"启用所有 MCP 功能（服务器使用和构建说明）",
						"仅启用 MCP 服务器使用（不包括构建 MCP 服务器的说明）",
						"禁用所有 MCP 功能"
					],
					"default": "full",
					"description": "控制提示中的 MCP 包含内容，如果您只需要访问特定功能，可以减少令牌使用量。"
				},
				"GI.enableCheckpoints": {
					"type": "boolean",
					"default": true,
					"description": "允许扩展在任务过程中保存工作区的检查点。"
				},
				"GI.disableBrowserTool": {
					"type": "boolean",
					"default": false,
					"description": "禁用扩展启动浏览器会话的功能。"
				},
				"GI.modelSettings.o3Mini.reasoningEffort": {
					"type": "string",
					"enum": [
						"low",
						"medium",
						"high"
					],
					"default": "medium",
					"description": "控制使用 o3-mini 模型时的推理努力程度。较高的值可能会产生更全面但响应更慢的结果。"
				},
				"GI.chromeExecutablePath": {
					"type": "string",
					"default": null,
					"description": "Chrome 可执行文件的路径，用于浏览器功能。如果未设置，扩展将尝试自动查找或下载。"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.js",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"vsec": "npm i && vsce package && ovsx publish",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run build:webview && npm run check-types && npm run lint && node esbuild.js --production",
		"compile-tests": "tsc -p ./tsconfig.test.json --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src --ext ts",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"test": "vscode-test",
		"install:all": "npm install && cd webview-ui && npm install",
		"dev:webview": "cd webview-ui && npm run dev",
		"build:webview": "cd webview-ui && npm run build",
		"test:webview": "cd webview-ui && npm run test",
		"publish:marketplace": "vsce publish && ovsx publish",
		"publish:marketplace:prerelease": "vsce publish --pre-release && ovsx publish --pre-release",
		"prepare": "husky",
		"changeset": "changeset",
		"version-packages": "changeset version"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.12",
		"@types/chai": "^5.0.1",
		"@types/diff": "^5.2.1",
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/should": "^11.2.0",
		"@types/vscode": "^1.84.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"chai": "^4.3.10",
		"esbuild": "^0.25.0",
		"eslint": "^8.57.0",
		"husky": "^9.1.7",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.3",
		"should": "^13.2.3",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.12.4",
		"@anthropic-ai/sdk": "^0.37.0",
		"@anthropic-ai/vertex-sdk": "^0.6.4",
		"@google-cloud/vertexai": "^1.9.3",
		"@google/generative-ai": "^0.18.0",
		"@haier/iam": "^1.6.0",
		"@mistralai/mistralai": "^1.5.0",
		"@modelcontextprotocol/sdk": "^1.0.1",
		"@types/clone-deep": "^4.0.4",
		"@types/get-folder-size": "^3.0.4",
		"@types/pdf-parse": "^1.1.4",
		"@types/turndown": "^5.0.5",
		"@vscode/codicons": "^0.0.36",
		"axios": "^1.7.4",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"claude-dev": "file:",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"execa": "^9.5.2",
		"fast-deep-equal": "^3.1.3",
		"firebase": "^11.2.0",
		"get-folder-size": "^5.0.0",
		"globby": "^14.0.2",
		"i": "^0.3.7",
		"ignore": "^7.0.3",
		"isbinaryfile": "^5.0.2",
		"mammoth": "^1.8.0",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
		"npm": "^11.1.0",
		"open-graph-scraper": "^6.9.0",
		"openai": "^4.83.0",
		"os-name": "^6.0.0",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"posthog-node": "^4.8.1",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.4.0",
		"serialize-error": "^11.0.3",
		"simple-git": "^3.27.0",
		"strip-ansi": "^7.1.0",
		"tree-sitter-wasms": "^0.1.11",
		"turndown": "^7.2.0",
		"vsce": "^2.15.0",
		"web-tree-sitter": "^0.22.6",
		"zod": "^3.24.2"
	}
}
